{
  "name": "simple-peer",
  "description": "Simple one-to-one WebRTC video/voice and data channels",
  "version": "9.1.2",
  "author": {
    "name": "Feross Aboukhadijeh",
    "email": "feross@feross.org",
    "url": "http://feross.org/"
  },
  "bugs": {
    "url": "https://github.com/feross/simple-peer/issues"
  },
  "dependencies": {
    "debug": "^4.0.1",
    "get-browser-rtc": "^1.0.0",
    "inherits": "^2.0.1",
    "readable-stream": "^2.3.4",
    "tiny-emitter": "^2.0.2"
  },
  "devDependencies": {
    "airtap": "0.1.0",
    "babel-minify": "^0.5.0",
    "bowser": "^1.9.2",
    "browserify": "^16.1.0",
    "electron-webrtc": "^0.3.0",
    "nyc": "^13.1.0",
    "prettier-bytes": "^1.0.3",
    "simple-get": "^3.0.1",
    "speedometer": "^1.0.0",
    "standard": "*",
    "string-to-stream": "^1.0.0",
    "tape": "^4.0.0",
    "through2": "^3.0.0",
    "thunky": "^1.0.1",
    "wrtc": "^0.2.0",
    "ws": "^6.0.0"
  },
  "keywords": [
    "data",
    "data channel",
    "data channel stream",
    "data channels",
    "p2p",
    "peer",
    "peer",
    "peer-to-peer",
    "stream",
    "video",
    "voice",
    "webrtc",
    "webrtc stream"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/simple-peer.git"
  },
  "scripts": {
    "build": "npm run build-debug && npm run build-stream && npm run build-lite",
    "build-debug": "node browserify.js > dist/simplepeer.js",
    "build-stream": "PRODUCTION=1 node browserify.js > dist/simplepeer.min.js",
    "build-lite": "PRODUCTION=1 LITE=1 node browserify.js > dist/simplepeer-lite.min.js",
    "size": "npm run build && cat simplepeer-lite.min.js | gzip | wc -c",
    "test": "standard src/** lite.js index.js test/** && npm run test-node && npm run test-browser",
    "test-browser": "airtap --coverage -- test/*.js",
    "test-browser-local": "airtap --coverage --local -- test/*.js",
    "test-node": "npm run test-node-electron-webrtc && npm run test-node-wrtc",
    "test-node-electron-webrtc": "WRTC=electron-webrtc tape test/*.js",
    "test-node-wrtc": "WRTC=wrtc tape test/*.js",
    "test-coverage": "nyc report --reporter=html"
  }
}
